#Tabela de correlação região e tipo de alimenração de morcegos
> batfood <- matrix(c(28, 5, 13, 45, 0, 110, 0, 5, 238, 45),
+ nrow = 2,
+ byrow = TRUE)
> #Nomear linhas e colunas
> print(batfood)
     [,1] [,2] [,3] [,4] [,5]
[1,]   28    5   13   45    0
[2,]  110    0    5  238   45
> rownames(batfood) <- c("Nectarívoro", "Frugívoro")
> colnames(batfood) <- c("Norte", "Nordeste", "Centro-Oeste", "Sudeste", "Sul")
> #aplicar o teste qui-quadrado
> teste <- chisq.test(batfood)
Warning message:
In chisq.test(batfood) : Aproximação do qui-quadrado pode estar incorreta
> #Mostrar
> print(teste)
        Pearson's Chi-squared test

data:  batfood
X-squared = 67.944, df = 4, p-value = 6.165e-14

